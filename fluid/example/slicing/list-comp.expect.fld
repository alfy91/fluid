let data = [{year: 2013, country: "China", energyType: "Bio", output: 6.2}, {year: 2013, country: "China", energyType: _"Hydro"_, output: 260}, {year: 2013, country: "China", energyType: "Solar", output: 19.9}, {year: 2013, country: "China", energyType: "Wind", output: 91}]
in [row.output | type <- _[_"Bio"_,_ _"Hydro"__,_ "Solar"_,_ "Wind"], row <- data, (row.energyType == type)]