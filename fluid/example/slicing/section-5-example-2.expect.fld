let map f [] = [];
    map f (x : xs) = (f x : map f xs)
in let data = [{year: 2013, country: "China", energyType: "Bio", output: _6.2_}, {year: 2013, country: "China", energyType: "Hydro", output: _260_}, {year: 2013, country: "China", energyType: "Solar", output: _19.9_}, {year: 2013, country: "China", energyType: "Wind", output: _91_}];
       output = [row.output | type <- ["Bio", "Hydro", "Solar", "Wind"], row <- data, (row.energyType == type)]
   in map fun x = floor ((x / sum output) * _100_) output