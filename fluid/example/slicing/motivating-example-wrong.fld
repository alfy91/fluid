let country_gdp country cities =
        sum [city.gdp | city <- cities, elem city.name country.cities];
let germany = {
        cities: ["Berlin", "Hamburg", "Munich"]
    };
let all_cities = [
        {name: "Berlin", gdp: 100, country: "Germany"},
        {name: "Munich", gdp: 80,  country: "Germany"},
        {name: "Hamburg", gdp: 60, country: "Germany"},
        {name: "London", gdp: 200, country: "UK"}
    ]
in country_gdp germany all_cities
