let map f [] = [];
    map f (x : xs) = (f x _:_ map f xs)
in let data = [{year: 2013, country: "China", energyType: "Bio", output: 6.2}, {year: 2013, country: "China", energyType: _"Hydro"_, output: 260}, {year: 2013, country: "China", energyType: "Solar", output: 19.9}, {year: 2013, country: "China", energyType: _"Wind"_, output: 91}];
       output = _[row.output | type <- ["Bio", _"Hydro"_, "Solar", _"Wind"_], row <- data, (row.energyType == type)]_
   in map fun x = floor ((x / sum output) * 100) output