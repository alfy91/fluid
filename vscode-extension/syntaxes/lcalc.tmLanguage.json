{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "LambdaCalc",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.lcalc",
				"match": "\\b(let|letrec|match|fun|in)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.lcalc",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.lcalc",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.lcalc"
}